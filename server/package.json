{
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run build && npm run readme",
    "readme": "ts-node utils/generateMarkdown.ts",
    "build": "npm run build-ts",
    "start": "node dist/index.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "serve-debug": "nodemon --inspect dist/index.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run watch-ts\" \"npm run serve-debug\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-retry": "3.1.2",
    "bluebird": "^3.5.5",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "^8.0.0",
    "koa": "2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "mysql": "^2.17.1",
    "pdfkit": "^0.10.0",
    "puppeteer": "^1.15.0",
    "sharp": "^0.22.1",
    "winston": "3.2.1",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/cheerio": "^0.22.11",
    "@types/dotenv": "^6.1.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.40",
    "@types/lodash": "^4.14.134",
    "@types/mustache": "^0.8.32",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.4",
    "@types/pdfkit": "^0.7.36",
    "@types/puppeteer": "^1.12.4",
    "@types/sharp": "^0.22.2",
    "@types/supertest": "^2.0.7",
    "axios-mock-adapter": "1.16.0",
    "nodemon": "^1.19.1",
    "supertest": "4.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.1"
  },
  "snyk": true
}
